<launch>
    <include file="$(find rslidar_pointcloud)/launch/rs_lidar_16.launch" >
    </include>
    <include file="$(find pointcloud_to_laserscan)/launch/xiaoqiang_rslidar.launch" >
    </include>

     <node pkg="tf" type="static_transform_publisher" name="base_link_to_laser" 
     args="0.3 0.0 0.20 0.0 0.0 0.0 base_link rslidar 100"/>

     <node pkg="laser_scan_matcher" type="laser_scan_matcher_node"
     name="laser_scan_matcher_node" output="screen">
     <param name="fixed_frame" value="odom"/>
     <param name="max_iterations" value="10"/>

     <param name="base_frame" value="base_link"/>
     <param name="use_odom" value="false" />
     <param name="publy_pose" value = "true" />
     <param name="publy_tf" value="true" />
     </node>
</launch>
